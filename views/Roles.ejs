<!DOCTYPE html>
<html>
<head>
	<title>Welcome!</title>
	<style type="text/css">
	</style>
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/animate.css@3.5.2/animate.min.css">
	<%- include("partials/head.ejs")%>
</head>
<body style="background-color: #222222">
	<%- include ("partials/server-navbar.ejs",{user,checkforperm}) %>

      <div class="container table-responsive-md">
        <table class="table table-dark bg-light">
          <thead class="thead-dark">
            <tr>
              <th scope="col"></th>
              <th scope="col">Adı</th>
              <th scope="col">ID</th>
              <th scope="col">Üye sayısı</th>
              <th scpoe="col">Renk</th>
              <th scope="col">Etiketlenebilir mi?</th>
              <th scope="col">Oluşturulduğu tarih</th>
            </tr>
          </thead>
          <tbody>
            <%var c = 0%>
            <% serverData.roles.cache.filter(r => r.name !== "@everyone").forEach((r,i) => { %>
              <% c ++%>
              <tr>
                <% moment.locale("tr")%>
                <th scope="row" style="color: wheat"><%= c%></th>
                <td scope="row" style="color: wheat">"><%=r.name%></td>
                <td scope="row" style="color: wheat">"><%=r.id%> </td>
                <td scope="row" style="color: wheat"><%=r.members.size%></td>
                <td scope="cell" style="color:<%=r.hexColor%>"><%=r.hexColor%></td>
                <td scope="row" style="color: wheat"><%=r.mentionable ? "Evet" : "Hayır"%></td>
                <td scope="row"  style="color: wheat"><%=moment(r.createdAt).format("LLL")%></td>
              </tr>
            <% }); %>
          </tbody>
        </table>
      </div>
      
</body>
